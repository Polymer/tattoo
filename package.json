{
  "name": "tattoo",
  "version": "1.0.2",
  "description": "Test all the things over and over",
  "main": "lib/tattoo.js",
  "bin": {
    "tattoo": "lib/tattoo.js"
  },
  "scripts": {
    "format": "find src custom_typings | grep '\\.ts$' | xargs clang-format --style=file -i",
    "test": "tslint src/*.ts src/**/*.ts custom_typings/*.ts",
    "build": "tsc",
    "tattoo": "node lib/tattoo.js",
    "build:watch": "tsc -w",
    "test:watch": "watch 'npm test' ./",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PolymerLabs/tattoo.git"
  },
  "keywords": [
    "bot"
  ],
  "author": "The Polymer Project Authors",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/PolymerLabs/tattoo/issues"
  },
  "dependencies": {
    "bottleneck": "^1.10.2",
    "bower": "^1.7.7",
    "command-line-args": "^2.0.4",
    "dom5": "^1.3.1",
    "github": "^0.2.0",
    "hydrolysis": "^1.19.3",
    "nodegit": "^0.12.2",
    "pad": "^1.0.0",
    "progress": "^1.1.8",
    "promisify-node": "^0.3.0",
    "resolve": "^1.1.7",
    "rimraf": "^2.4.3",
    "semver": "^5.1.0"
  },
  "devDependencies": {
    "@types/progress": "^1.1.8",
    "@types/rimraf": "^0.0.28",
    "@types/semver": "^5.1.0",
    "clang-format": "=1.0.45",
    "tslint": "^3.10.2",
    "typescript": "^2.0.3",
    "watch": "^0.17.1"
  },
  "optionalDependencies": {
    "source-map-support": "^0.4.0"
  }
}
